<lifecycles>
  <lifecycle>
    <id>ddu</id>
    <phases>
      <phase>
        <id>initialize</id>
        <executions>
          <execution>
            <goals>
              <goal>init</goal>
            </goals>
          </execution>
        </executions>
      </phase>
      <phase>
        <id>test</id>
        <configuration>
        <testFailureIgnore>true</testFailureIgnore>
          <properties>
            <property>
              <name>listener</name>
              <value>pt.up.fe.ddu.base.runtime.TestListener</value>
            </property>
          </properties>
        </configuration>
      </phase>
    </phases>
  </lifecycle>
</lifecycles>
